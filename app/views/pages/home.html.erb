<div class="container d-flex justify-content-center flex-column">
  <h1>List-It</h1>
    <% if !current_user %>
      <%= link_to "Login", new_user_session_path %>
    <% else %>
      <% @lists.each do |list| %>
        <ul>
          <li><%= link_to list.title, list  %> - <em> <%= time_ago_in_words(list.created_at) %> </em> - <%= link_to format_username(list.user.username), list.user %>
          </li>
        </ul>
      <% end %>
    <% end %>
</div>
