<h1>My Lists</h1>

  <ul>
<% @lists.each do | list | %>
    <li><%= link_to list.title, user_list_path(current_user, list) %>
      <% list.tag_list.each do |tag| %>
        <%= link_to tag, tagged_path(tag: tag) %>
      <% end %>
    </li>
  <% end %>
  </ul>

