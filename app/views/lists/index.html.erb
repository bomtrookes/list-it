<div class="container">
  <h1>Discover</h1>
  <ul>
    <% @ordered_lists.each do | list | %>
     <li class="d-flex justify-content-between">
          <%= link_to list.title, user_list_path(current_user, list) %>
            <i class="fa-solid fa-thumbs-up"></i>
          <%= list.votes.size ? list.votes.size : 0 %>
          </li>
    <% end %>
  </ul>

</div>
