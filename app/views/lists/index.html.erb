<div class="container">
  <h1>Lists</h1>

    <ul>
  <% @user.lists.each do | list | %>
      <li class="d-flex justify-content-between">
      <%= link_to list.title, user_list_path(current_user, list) %>
      <%= list.votes ? list.votes : "new" %>
      </li>
    <% end %>
    </ul>

  <br>
  <br>

  <% if hide_button(@user)%>
    <%= link_to "+ Create List", new_user_list_path  %>
  <% end %>
</div>
